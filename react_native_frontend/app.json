        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.raidenblackout.animefy"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "d35381ab-0973-462e-99d4-e4ff8979fb64"
      }
    },
    "owner": "raidenblackout"
  }
